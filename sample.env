# RekognizeVote - Configuração de Ambiente

# URLs da API
BASE_URL_DEV=https://dev-api.rekognizevote.com/
BASE_URL_PROD=https://api.rekognizevote.com/

# AWS Configuration (se necessário para configurações específicas)
AWS_REGION=us-east-1

# Configurações de Debug
DEBUG_LOGGING=true
MOCK_DATA=true

# Configurações de Reconhecimento Facial
FACE_SIMILARITY_THRESHOLD=90.0
MAX_IMAGE_SIZE_MB=5

# Configurações de Timeout (em segundos)
NETWORK_TIMEOUT=30
CAMERA_TIMEOUT=30

# Configurações de Cache
CACHE_SIZE_MB=50
CACHE_DURATION_HOURS=24

# Configurações de Segurança
ENABLE_CERTIFICATE_PINNING=true
ENABLE_ROOT_DETECTION=false

# Configurações de Analytics (se aplicável)
ENABLE_ANALYTICS=false
ANALYTICS_API_KEY=your_analytics_key_here

# Configurações de Crash Reporting (se aplicável)
ENABLE_CRASH_REPORTING=false
CRASH_REPORTING_API_KEY=your_crash_reporting_key_here